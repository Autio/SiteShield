{
    "name": "SiteShield",
    "description": "Build an Extension!",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["common.js"]
      },
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ]
}